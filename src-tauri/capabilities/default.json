{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://localhost:5000"
        },
        {
          "url": "https://192.168.1.70:5000"
        }
      ]
    },
    "websocket:default",
    "websocket:default"
  ]
}